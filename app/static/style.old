                    /* *******##### BUTTONS #####******* */

/* ***##### REGISTER BUTTON #####*** */

.signup-button {
display: block;
margin: auto;
margin-top: 25px;
margin-bottom: 20px;
background-color: #141414;
color: #10CDE6;
padding: 8px 73px;
border: 1px solid #10CDE6;
border-radius: 20px;
cursor: pointer;
font-size: 16px;
transition: background-color 0.2s ease-out, transform 0.2s ease-out;
font-family: "Plus Jakarta Sans", "Noto Sans", sans-serif;
font-weight: bold;

}

.signup-button:hover {
background-color: #10CDE6;
border: 1px solid #10CDE6;
color: #050505;
transform: scale(1.03);

}

.signup-button:active {
transform: scale(0.95);

}


