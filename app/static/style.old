                    /* *****### ANOTHER GENERAL SETTINGS ###***** */


/* ***##### DEVICES #####*** */

@media (max-width: 580px) {
    .toggle-group {
        vertical-align: center;
        flex-direction: row;
        align-items: flex-start;
    }
}

input[type="number"]::-webkit-inner-spin-button,
input[type="number"]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    
    margin: 0;
}

input[type="number"] {
    -moz-appearance: textfield;
}

.file-upload-wrapper {
    position: relative;
    display: inline-block;
    width: 100%;
}

/* Ocultar el input file original pero mantenerlo funcional */
.file-upload-wrapper input[type="file"] {
    position: absolute;
    opacity: 0;
    width: 100%;
    height: 100%;
    cursor: pointer;
    z-index: 2;
}

/* SELECT LISTS */

.select-container {
    display: flex;
    justify-content: center;
    margin: 20px 0;
}

.select-list {
  
    text-indent: 8px;
    width: 222px;
    height: 35px;
    display: block;
    border-radius: 25px;
    text-align: left;
    font-size: 15px;
    font-weight: bold;
    border: 1px solid transparent;
    border-radius: 25px;
    background-color: #333333;
    color: #CCCCCC;
    cursor: pointer;
    transition: all 0.3s ease;
}


.select-list:focus {
    outline: none;
    border-color: #10CDE6;
    /* box-shadow: 0 0 0 3px rgba(99, 179, 237, 0.25); */
}

.select-list option {
    
    
    background-color: #141414;
    color: #CCCCCC;
    border-radius: 25px;
}
.calendar-container {
    display: flex;
    justify-content: center;
    margin: 20px 0;
}

.calendar {
  

    text-align: left;
    text-indent: 8px;
    font-size: medium;
    width: 215px;
    height: 30px;
    border: 2px solid transparent;
    margin-bottom: 5px;
    border-radius: 15px;
