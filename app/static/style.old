}

.file-upload-container {
    text-align: center;
    padding: 20px;
    margin: 20px auto;
    max-width: 400px;
    position: relative;
}

.file-upload-input {
    display: none;
}

.file-upload-button {
    display: inline-block;
    padding: 8px 50px;
    margin-top: 50px;
    background-color: transparent;
    color: #10CDE6;
    border: 1px solid #10CDE6;
    border-radius: 25px;
    cursor: pointer;
    font-size: 16px;
    transition: background-color 0.4s ease, transform 0.2s ease;
}

.file-upload-button:hover {
    background-color: #10CDE6;
    border: 1px solid #10CDE6;
    color: #050505;
    transform: scale(1.03);   
}

.file-upload-input:valid + .file-upload-button {
    background-color: #4caf50;
    border-color: #4caf50;
    color: #141414;
}

.file-upload-input:valid + .file-upload-button::after {
    content: " âœ“";
}


